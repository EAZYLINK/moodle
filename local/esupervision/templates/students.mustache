{{#issupervisor}}
<div style="height: 300px; overflow-y: auto;">
<table class="table">
    <caption class="text-center">
        Assigned Project Students
    </caption>
    <tbody>
        <tr>
            <th>ID</th>
            <th>Group Name</th>
            <th>Supervisor Name</th>
            <th>Student Name</th>
            <th>Action</th>
        </tr>
        {{#students}}
        <tr>
            <td>{{ id }}</td>
            <td>{{ group }}</td>
            <td>{{ supervisor}}</td>
            <td>{{ student }}</td>
            <td style="white-space: nowrap;">
                <a href="?action=edit&id={{ id }}" class="btn"><img src="assets/edit.png"></a>
                <a href="?action=delete&id={{ id }}" class="btn"><img src="assets/delete.png"></a>
            </td>
        </tr>
        {{/students}}
    </tbody>
</table>
</div>
{{/issupervisor}}

{{#isstudent}}
<div class="container">

</div>
{{/isstudent}}